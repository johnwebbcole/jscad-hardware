{
    "env": {
        "browser": true
    },
    "globals": {
        "CSG": true,
        "CAG": true,
        "echo": true,
        "util": true,
        "include": true,
        "union": true,
        "Colors": true,
        "RaspberryPi": true,
        "Boxes": true,
        "Parts": true,
        "_": true,
        "vector_text": true,
        "rectangular_extrude": true,
        "_boardutils": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": [2, {"allow": ["warn", "error"]}],
        "linebreak-style": [
            2,
            "unix"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ]
    }
}
